{
  "name": "dokployaction",
  "version": "1.0.0",
  "description": "Complete Dokploy deployment automation for GitHub Actions",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "build:watch": "ncc build src/index.ts -o dist --source-map --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "all": "npm run format && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SSanjeevi/dokployaction.git"
  },
  "keywords": [
    "dokploy",
    "deployment",
    "github-actions",
    "docker",
    "container",
    "devops",
    "ci-cd"
  ],
  "author": "SSanjeevi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SSanjeevi/dokployaction/issues"
  },
  "homepage": "https://github.com/SSanjeevi/dokployaction#readme",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@actions/http-client": "^2.2.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
